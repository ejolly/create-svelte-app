{
    "files.associations": {
      "*.svelte": "html"
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      {
        "language": "html",
        "autoFix": true
      }
    ],
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.lineHeight": 19,
    "editor.formatOnSave": true
  }